<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean class="io.ravneet.springbean.Employee" name="employee">
        <constructor-arg name="fname" value="R"></constructor-arg>
        <constructor-arg name="lname" value="K"></constructor-arg>
        <constructor-arg name="company" value="Google"></constructor-arg>
    </bean>
</beans>